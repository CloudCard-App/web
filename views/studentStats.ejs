<html>
<head>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
</head>
<head>
    <div class="col-sm-8">
        <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
        <script type="text/javascript">
            google.charts.load('current', {'packages': ['corechart']});
            google.charts.setOnLoadCallback(drawChart);

            function drawChart() {
                var data = google.visualization.arrayToDataTable(<%-JSON.stringify(graphData)%>, false);

                var options = {
                    title: "Students Over Time",
                    legend: {position: 'bottom'}
                };
                var chart = new google.visualization.BarChart(document.getElementById('curve_chart'));
                <!-- For a line chart, change this   ^^^ to Line -->

                chart.draw(data, options);
            }
        </script>
    </div>
<body>
<div class="container">
    <div class="page-header text-center">
        <h1>Student Studying Statistics for deck <%- JSON.stringify(deckName) %></h1>
    </div>
</div>

<div id="curve_chart" style="width: 900px; height: 500px"></div>

</body>

</head>
</html>
